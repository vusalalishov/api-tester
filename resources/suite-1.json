{
  "title": "Test suite containing user management system tests",
  "label": ["suite-1"],
  "protocol": "json",
  "declare": {
    "userName": "John Doe",
    "userAge": "30",
    "baseUrl": "http://localhost"
  },
  "cases": [
    {
      "title": "Test if user created successfully",
      "labels": ["case-1"],
      "scenarios": [
        {
          "title": "Create user!",
          "try": {
            "method": "POST",
            "url": "http://localhost:3000/create",
            "headers": {
              "key1": [
                "value1",
                "value2"
              ],
              "key3": "value3"
            },
            "payload": {
              "name": "${userName}",
              "age": "${userAge}"
            }
          },
          "verify": {
            "status": "200",
            "headers": {
              "key1": ["value1", "value2"],
              "key3": "value3"
            },
            "schema": {
              "errorCode": "0",
              "errorMessage": "",
              "data": {
                "userId": "1"
              }
            }
          }
        }
      ]
    },
    {
      "title": "Test if user retrieved successfully",
      "labels": ["case-2"],
      "scenarios": [
        {
          "title": "Get user!",
          "try": {
            "method": "GET",
            "url": "http://localhost:3000/get/1"
          },
          "verify": {
            "status": "200",
            "headers": {
              "key1": ["value1", "value2"],
              "key3": "value3"
            },
            "schema": {
              "errorCode": "0",
              "errorMessage": "",
              "data": {
                "userId": "1",
                "name": "John Doe",
                "age": "15",
                "phoneNumbers": [
                  "1",
                  "2",
                  "3"
                ]
              }
            }
          }
        }
      ]
    }
  ]
}